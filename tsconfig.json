{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": [
      "cypress",
      "node"
    ],
    "allowJs": true,
    "checkJs": false,
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": [
    "**/*.ts"
  ]
}
